@model PlaybackSessionListViewModel;
@{
	ViewData["Title"] = "Playback Sessions";
}


@foreach (var session in Model.PlaybackSessions)
{
<h3>@session.DiscordVoiceChannelId</h3>
<table class="table">
	<thead>
		<tr>
			<th scope="col">Email</th>
			<th scope="col">Discord User Id</th>
			<th scope="col">Registration Status</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var user in session.JoinedUsers)
			{
	<td>@user.Email</td>
	<td>@user.DiscordUserId</td>
	<td>@user.DiscordUser.UserStatus</td>
		}

		</tbody>
	</table>

	}